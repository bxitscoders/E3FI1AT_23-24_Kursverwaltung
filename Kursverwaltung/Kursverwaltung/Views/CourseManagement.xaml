        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kursverwaltung.Views"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
